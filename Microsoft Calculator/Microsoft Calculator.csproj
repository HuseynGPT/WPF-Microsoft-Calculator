<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Microsoft_Calculator</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\icons8-menu-24.png" />
    <None Remove="Resources\icons8_calculator.ico" />
    <None Remove="Resources\time_machine_24px.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\icons8-menu-24.png" />
    <Resource Include="Resources\icons8_calculator.ico" />
    <Resource Include="Resources\time_machine_24px.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
